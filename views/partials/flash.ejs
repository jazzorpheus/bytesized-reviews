<% for (let flash of flashMsgs) { %>
<% if (flash.msg.length) { %>
<div class="alert alert-<%=flash.alertType%> alert-dismissible fade show" role="alert">
    <strong>
        <%= flash.msg %>
    </strong>
    <%= flash.secondary %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
<% } %>